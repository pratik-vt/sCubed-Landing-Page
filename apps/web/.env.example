# Next.js Environment Variables
# Copy this file to .env.local and fill in your values

# Strapi CMS
NEXT_PUBLIC_STRAPI_URL=http://localhost:1337

# Admin App
NEXT_PUBLIC_ADMIN_APP_URL=
NEXT_PUBLIC_ADMIN_APP_API_URL=
NEXT_PUBLIC_GUARDIAN_APP_URL=

# Environment
NEXT_PUBLIC_APP_ENV=dev

# Google Tag Manager
NEXT_PUBLIC_GTM_ID=

# Calendly
NEXT_PUBLIC_CALENDLY_URL=

# Site URL
NEXT_PUBLIC_SITE_URL=

# Social Media
NEXT_PUBLIC_FACEBOOK_URL=
NEXT_PUBLIC_INSTAGRAM_URL=
NEXT_PUBLIC_YOUTUBE_URL=

# Contact Information
NEXT_PUBLIC_PHONE_NUMBER=
NEXT_PUBLIC_EMAIL=
NEXT_PUBLIC_ADDRESS=

# reCAPTCHA (v2 Invisible)
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET_KEY=

# reCAPTCHA v3 (Invisible)
NEXT_PUBLIC_RECAPTCHA_SITE_KEY_V3=
RECAPTCHA_SECRET_KEY_V3=

# Mailchimp Newsletter Integration
# Get these from your Mailchimp account:
# - API Key: Account -> Extras -> API Keys
# - Server Prefix: The part before .api.mailchimp.com in your API endpoint (e.g., us21)
# - List ID: Audience -> Settings -> Audience name and defaults
MAILCHIMP_API_KEY=
MAILCHIMP_SERVER_PREFIX=
MAILCHIMP_LIST_ID=